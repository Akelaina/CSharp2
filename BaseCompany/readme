Создать БД с именем "lesson7_1713"

CREATE TABLE [Departments] (
    [ID]   INT        IDENTITY (1, 1) NOT NULL,
    [dName] NCHAR (20) DEFAULT (N'Новый_отдел_1') NULL,
    PRIMARY KEY CLUSTERED ([ID] ASC)
);



CREATE TABLE [Employees] (
    [ID]           INT        IDENTITY (1, 1) NOT NULL,
    [eName]         NCHAR (10) NOT NULL,
    [Surname]      NCHAR (10) NOT NULL,
    [Age]          TINYINT    DEFAULT ((22)) NULL,
    [Salary]       MONEY      DEFAULT ((50000)) NULL,
    [DepartmentID] INT        DEFAULT ((1)) NOT NULL,
    CONSTRAINT [PK_Employees] PRIMARY KEY CLUSTERED ([ID] ASC),
    CONSTRAINT [FK_DepartmentID] FOREIGN KEY ([DepartmentID]) REFERENCES [Departments] ([ID])
);


